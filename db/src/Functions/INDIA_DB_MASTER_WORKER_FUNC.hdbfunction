FUNCTION "INDIA_DB_MASTER_WORKER_FUNC"( CURR_IP NVARCHAR(3))
    RETURNS TABLE(
    "FIRSTNAME" NVARCHAR(30),
    "LASTNAME" NVARCHAR(30),
    "GENDER" NVARCHAR(1),
    "CURRENCY" NVARCHAR(3)
    )
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER AS
BEGIN
    RETURN 

SELECT "FIRSTNAME",
        "LASTNAME",
        "GENDER",
        "CURRENCY" 
        from 
        "INDIA_DB_MASTER_WORKER" 
        where CURRENCY = :CURR_IP;

END;