PROCEDURE "WORKER"(in CURR_IP nvarchar(3) )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
   SELECT * from "INDIA_DB_MASTER_WORKER" WHERE CURRENCY = :CURR_IP;
END

